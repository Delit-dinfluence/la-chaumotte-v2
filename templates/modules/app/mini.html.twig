        $(document).ready(function () {

            $('.single-item').slick({
                dots: true,
            });

            /***
             * Ouverture et fermeture de la modal + déclenchement de l'anim btn hamburger
             */
            $('#modal-command').on('click', function (event) {
                event.preventDefault;

                let $modal = $('#modal-navigation');
                let $modalCommand = $('#modal-command');
                let $navigation = $('#navigation');

                if ($modal.hasClass('active')) {

                    $modal.removeClass('active');
                    $modalCommand.removeClass('is-active');
                    $modalCommand.css('position', 'inherit');
                    $navigation.removeClass('indexed');

                } else {

                    $modal.addClass('active');
                    $modalCommand.addClass('is-active');
                    $modalCommand.css('position', 'fixed');
                    $navigation.addClass('indexed');
                }

            });

            $navigation = $('#navigation');

            $(window).scroll(function () {

                if ($(window).scrollTop() > 40) {

                    if (!$navigation.hasClass('active'))
                        $navigation.addClass('active');
                } else {
                    if ($navigation.hasClass('active'))
                        $navigation.removeClass('active');
                }
            });

            /**
             * Fermeture de la modal quand l'écran dépasse 900px
             */
            $(window).resize(function () {

                let $pageWidth = $(window).width();
                let $modal = $('#modal-navigation');
                let $modalCommand = $('#modal-command');

                if ($pageWidth > 900) {
                    $modal.removeClass('active');
                    $modalCommand.removeClass('is-active');
                }

            });
            /**
             * Fermeture de la modal quand au click sur la navigation
             */

            $('#modal-navigation .nav-link').on('click', function (event) {
                event.preventDefault();

                let $modal = $('#modal-navigation');
                let $modalCommand = $('#modal-command');

                $modal.removeClass('active');
                $modalCommand.removeClass('is-active');

            });

            /**
             * Fonction scroll to au click sur la navigation
             */
            $('.js-scrollTo').on('click', function () { // Au clic sur un élément
                var page = $(this).attr('href'); // Page cible
                var speed = 750; // Durée de l'animation (en ms)
                $('html, body').animate({scrollTop: $(page).offset().top - 40}, speed); // Go
                return false;
            });
        });

    </script>


