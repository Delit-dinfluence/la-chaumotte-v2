        var map;

        function initMap() {
            var myLatLng = {lat: 47.204926, lng: 2.410242};
            map = new google.maps.Map(document.getElementById('map'), {
                center: myLatLng,
                zoom: 9
            });


            var infowindow = [];


            var contentString = '<a href="#" class="infoWindow-link"><div id="content">' +
                '<p id="producteurName" class="producteur-name">{{ translation.page_mini_section_vendeurs_1_content_title|striptags }}</p>' +
                '<p id="producteurAdress" class="producteur-adress">{{ translation.pop_up_map_adresse1|raw }}</p></a>';


            infowindow[0] = new google.maps.InfoWindow({
                content: contentString,
                maxWidth: 200
            });


            var contentString = '<a href="#" class="infoWindow-link"><div id="content">' +
                '<p id="producteurName" class="producteur-name">{{ translation.page_mini_section_vendeurs_2_content_title|striptags }}</p>' +
                '<p id="producteurAdress" class="producteur-adress">{{ translation.pop_up_map_adresse2|raw }}</p></a>';

            infowindow[1] = new google.maps.InfoWindow({
                content: contentString,
                maxWidth: 200
            });


            var contentString = '<a href="#" class="infoWindow-link"><div id="content">' +
                '<p id="producteurName" class="producteur-name">{{ translation.page_mini_section_vendeurs_3_content_title|striptags }}</p>' +
                '<p id="producteurAdress" class="producteur-adress">{{ translation.pop_up_map_adresse3|raw }}</p></a>';


            infowindow[2] = new google.maps.InfoWindow({
                content: contentString,
                maxWidth: 200
            });


            var latlng1 = {lat: 47.136456, lng: 2.4303764};
            var marker1 = new google.maps.Marker({
                position: latlng1,
                map: map,
                title: 'producteur1',

            });

            marker1.addListener('mouseover', function () {
                infowindow[0].open(map, marker1);
            });

            marker1.addListener('mouseout', function () {
                infowindow[0].close(map, marker1);
            });

            marker1.addListener('click', function () {

                var element = document.getElementById("block-column1");
                var element2 = document.getElementById("block-column2");
                var element3 = document.getElementById("block-column3");

                if (element2.classList.contains("active")) {
                    element.classList.add("active");
                    element2.classList.remove("active");
                    element.scrollIntoView({behavior: 'smooth', block: 'end'});

                } else if ((element3.classList.contains("active"))) {
                    element.classList.add("active");
                    element3.classList.remove("active");
                    element.scrollIntoView({behavior: 'smooth', block: 'end'});

                } else if ((element.classList.contains("active"))) {
                    element2.classList.remove("active");
                    element.classList.add("active");
                    element.scrollIntoView({behavior: 'smooth', block: 'end'});

                } else {
                    element.classList.add("active");
                    element.scrollIntoView({behavior: 'smooth', block: 'end'});
