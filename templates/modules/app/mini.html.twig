{% extends '@app/base.html.twig' %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('build/mini.min.css') }}">
    <link type="text/css" rel="stylesheet"
          href="{{ asset('assets/vendors/animated-hamburger-1.1.3/css/hamburgers.min.css') }}">

    <link rel="stylesheet" type="text/css" href="{{ asset('assets/vendors/slick-1.8.1/css/slick.css') }}"/>
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,900&display=swap" rel="stylesheet">


{% endblock %}

{% block content %}

    <div class="page page-mini">

        <div class="navigation" id="navigation">

            <div class="container">

                {% if images['MINI_LOGO'] is defined and images['MINI_LOGO'].image != '' %}
                    <div class="column">
                        <a class="logo" href="#">
                            {% if images['MINI_LOGO'] is defined and images['MINI_LOGO'].image != '' %}
                                <img src="{{ images['MINI_LOGO'].slug }}" alt="{{ images['MINI_LOGO'].alt }}">
                            {% else %}
                                <img src="{{ images['NO_IMAGE'].slug }}" alt="{{ images['NO_IMAGE'].alt }}">
                            {% endif %}
                        </a>
                    </div> {# /.column #}
                {% endif %}

                <div class="column column-navigation">

                    <nav id="nav" class="nav">

                        <ul class="nav-list">

                            {% if translation.page_mini_navigation_link_1 is defined %}
                                <li class="nav-item">
                                    <a href="#about" class="nav-link js-scrollTo">
                                        {{ translation.page_mini_navigation_link_1|striptags }}
                                    </a>
                                </li>
                            {% endif %}

                            {% if translation.page_mini_navigation_link_2 is defined %}
                                <li class="nav-item">
                                    <a href="#lesProduits" class="nav-link js-scrollTo">
                                        {{ translation.page_mini_navigation_link_2|striptags }}
                                    </a>
                                </li>
                            {% endif %}

                            {% if translation.page_mini_navigation_link_3 is defined %}
                                <li class="nav-item">
                                    <a href="#lesPointsDeVente" class="nav-link js-scrollTo">
                                        {{ translation.page_mini_navigation_link_3|striptags }}
                                    </a>
                                </li>
                            {% endif %}

                            {% if translation.page_mini_navigation_link_4 is defined %}
                                <li class="nav-item">
                                    <a href="#passerCommande" class="nav-link js-scrollTo">
                                        {{ translation.page_mini_navigation_link_4|striptags }}
                                    </a>
                                </li>
                            {% endif %}

                            {% if translation.page_mini_navigation_link_5 is defined %}
                                <li class="nav-item">
                                    <a href="#" class="nav-link">
                                        {{ translation.page_mini_navigation_link_5|striptags }}
                                    </a>
                                </li>
                            {% endif %}

                        </ul> {# /.nav-list #}

                    </nav> {# /.nav #}

                </div> {# /.column #}

                <div class="column column-hamburger">

                    <div class="open-modal">

                        <button class="hamburger burger hamburger--squeeze" type="button" id="modal-command">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </button>

                    </div> {# /.open-modal #}

                </div> {# /.column #}

            </div> {# /.container #}

        </div> {# /.navigation #}

        <header id="page-header" class="page-header"
