    <script src="{{ asset('assets/vendors/slick-1.8.1/js/slick.min.js') }}"></script>

    <script>

        // setTimeout(function () {
        //     $('#modal-cancel').fadeIn('fade').addClass('active');
        // }, 1750);
        //
        // $('#modal-clsoe').on('click', function (event) {
